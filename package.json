{
  "name": "zillion-whales-smart-contracts",
  "version": "0.0.1",
  "scripts": {
    "checkPacks": "node ./bin/calculateGeneratedPacks.js",
    "uploadImages": "node ./bin/cli.js uploadImages -p '../LordImages'",
    "mintLords:test": "node ./bin/cli.js mintLords -e 'test' -p 'WF_Community_Sale_Lords.csv'",
    "mintPacks:test": "node ./bin/cli.js mintPacks -e 'test' -p 'releaseSalePacksData.json' -l 'NewConfigs - LordsMetadata.csv'",
    "mintLords:prod": "node ./bin/cli.js mintLords -e 'prod' -p 'WF_Community_Sale_Lords'",
    "mintPacks:prod": "node ./bin/cli.js mintPacks -e 'prod' -p 'prodPacksData.json' -l 'minted_lords-prod.json'",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat deploy",
    "deploy:test": "npx hardhat deploy --network saigon",
    "deploy:test:nftUnits": "npx hardhat deploy --network saigon --tags nftUnits",
    "deploy:test:nftSkins": "npx hardhat deploy --network saigon --tags nftSkins",
    "deploy:test:nftLords": "npx hardhat deploy --network saigon --tags nftLords",
    "deploy:test:nftPacks": "npx hardhat deploy --network saigon --tags nftPacks1155",
    "deploy:test:token": "npx hardhat deploy --network saigon --tags token",
    "deploy:test:nftClaimUnits": "npx hardhat deploy --network saigon --tags nftClaimUnits",
    "deploy:test:nftClaimUnitsBeta": "npx hardhat deploy --network saigon --tags nftClaimUnitsBeta",
    "deploy:test:nftClaimSkins": "npx hardhat deploy --network saigon --tags nftClaimSkins",
    "deploy:test:tokenClaimTransfer": "npx hardhat deploy --network saigon --tags tokenClaimTransfer",
    "deploy:test:nftLock": "npx hardhat deploy --network saigon --tags nftLock",
    "deploy:prod": "npx hardhat deploy --network ronin",
    "deploy:prod:nftUnits": "npx hardhat deploy --network ronin --tags nftUnits",
    "deploy:prod:nftSkins": "npx hardhat deploy --network ronin --tags nftSkins",
    "deploy:prod:nftLords": "npx hardhat deploy --network ronin --tags nftLords",
    "deploy:prod:nftPacks": "npx hardhat deploy --network ronin --tags nftPacks1155",
    "deploy:prod:token": "npx hardhat deploy --network ronin --tags token",
    "deploy:prod:nftClaimUnits": "npx hardhat deploy --network ronin --tags nftClaimUnits",
    "deploy:prod:nftClaimUnitsBeta": "npx hardhat deploy --network ronin --tags nftClaimUnitsBeta",
    "deploy:prod:nftClaimSkins": "npx hardhat deploy --network ronin --tags nftClaimSkins",
    "deploy:prod:tokenClaimTransfer": "npx hardhat deploy --network ronin --tags tokenClaimTransfer",
    "deploy:prod:nftLock": "npx hardhat deploy --network ronin --tags nftLock",
    "verify:test": "npx hardhat --network saigon sourcify --endpoint https://sourcify.roninchain.com/server",
    "verify:prod": "npx hardhat --network ronin sourcify --endpoint https://sourcify.roninchain.com/server",
    "gasreport": "npx hardhat test",
    "test": "npx hardhat coverage --show-stack-traces",
    "test:scripts": "NODE_ENV=test nyc --reporter=lcov --reporter=text _mocha ./testScripts/*.js --exit --timeout 35000",
    "lint": "eslint . && prettier . --check && solhint 'contracts/**/*.sol'",
    "format": "prettier . --write && eslint . --fix"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.4",
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@openzeppelin/hardhat-upgrades": "1.28.0",
    "base64-sol": "^1.1.0",
    "chai": "^4.3.7",
    "csvtojson": "^2.0.10",
    "deep-equal-in-any-order": "^2.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.5",
    "hardhat-deploy": "^0.11.12",
    "hardhat-gas-reporter": "^1.0.9",
    "inquirer": "^8.0.0",
    "lodash.chunk": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.4",
    "yargs": "^17.7.1"
  },
  "engines": {
    "npm": ">=9.6.4",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@thirdweb-dev/storage": "^2.0.8",
    "json-2-csv": "^5.0.1",
    "lodash.groupby": "^4.6.0",
    "lodash.keyby": "^4.6.0"
  }
}
